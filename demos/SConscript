import os

env=DefaultEnvironment().Clone();

env.Library('resource', ['resource.c']);

env['LIBS'] = ['ui_loader', 'resource', 'fontbitmap', 'imageloaderbitmap'] + env['LIBS']
env['LINKFLAGS'] = env['OS_SUBSYSTEM_WINDOWS'] + env['LINKFLAGS'];

env.Program('demo1', ['demo_main.c', 'demo1_app.c']);
env.Program('demoui', ['demo_ui_app.c', 'demo_main.c']);

env['LIBS'] = ['xml'] + env['LIBS']
env.Program('prefix_xml_ui', ['prefix_xml_ui.c']);




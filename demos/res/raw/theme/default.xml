<window>
  <style name="default">
    <normal bg_color="#f0f0f0"/>
  </style>

  <style name="1:bricks">
    <normal bg_image="bricks"  bg_image_draw_type="repeat"/>
  </style>

  <style name="2:sky">
    <normal bg_image="bg800x480"/>
  </style>
</window>

<button>
  <style name="default" border_color="#a0a0a0"  text_color="black">
    <normal     bg_color="#f0f0f0" />
    <pressed    bg_color="#c0c0c0" />
    <over       bg_color="#e0e0e0" />
    <disable    bg_color="gray" text_color="#d0d0d0" />
  </style>
  
  <style name="2:spinbox_down" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_down_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_down_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_down_o"/>
  </style>

  <style name="3:spinbox_up" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_up_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_up_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_up_o"/>
  </style>
  
  <style name="4:spinbox_left" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_left_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_left_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_left_o"/>
  </style>
  
  <style name="5:spinbox_right" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_right_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_right_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_right_o"/>
  </style>

  <style name="6:edit_clear">
    <normal     icon="edit_clear_n"/>
    <pressed    icon="edit_clear_p"/>
    <over       icon="edit_clear_o"/>
  </style>

  <style name="10:grid_item" border_color="#a0a0a0"  text_color="black">
    <pressed    bg_color="#c0c0c0" />
  </style>
  
  <style name="11:scroll_down" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_down_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_down_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_down_o"/>
  </style>

  <style name="12:scroll_up" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_up_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_up_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_up_o"/>
  </style>
  
  <style name="13:scroll_left" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_left_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_left_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_left_o"/>
  </style>
  
  <style name="14:scroll_right" border_color="#a0a0a0">
    <normal     bg_color="#f0f0f0" icon="arrow_right_n"/>
    <pressed    bg_color="#c0c0c0" icon="arrow_right_p"/>
    <over       bg_color="#e0e0e0" icon="arrow_right_o"/>
  </style>
</button>

<edit>
  <style name="default" border_color="#a0a0a0"  text_color="black" text_align_h="left">
    <normal     bg_color="#f0f0f0" />
    <focused    bg_color="#f0f0f0" border_color="black"/>
    <disable    bg_color="gray" text_color="#d0d0d0" />
    <error      bg_color="#f0f0f0" text_color="red" />
    <empty      bg_color="#f0f0f0" text_color="#a0a0a0" />
  </style>
  <style name="1:line" border="bottom" border_color="#a0a0a0"  text_color="black" text_align_h="left" text_align_v="bottom">
    <normal     bg_color="#f0f0f0" />
    <focused    bg_color="#f0f0f0" border_color="black"/>
    <disable    bg_color="gray" text_color="#d0d0d0" />
    <error      bg_color="#f0f0f0" text_color="red" />
    <empty      bg_color="#f0f0f0" text_color="#a0a0a0" />
  </style>
</edit>

<spin_box>
  <style name="default" border_color="#a0a0a0"  text_color="black" text_align_h="left">
    <normal     bg_color="#f0f0f0" />
    <focused    bg_color="#f0f0f0" border_color="black"/>
    <disable    bg_color="gray" text_color="#d0d0d0" />
    <error      bg_color="#f0f0f0" text_color="red" />
    <empty      bg_color="#f0f0f0" text_color="#a0a0a0" />
  </style>
</spin_box>

<label>
  <style name="default">
    <normal text_color="black" />
  </style>

  <style name="1:green">
    <normal text_color="green" />
  </style>

  <style name="2:left">
    <normal text_color="red" text_align_h="left" border_color="#a0a0a0" margin="4" />
  </style>

  <style name="3:center">
    <normal text_color="green" text_align_h="center" border_color="#a0a0a0"/>
  </style>
  
  <style name="4:right">
    <normal text_color="blue" text_align_h="right" border_color="#a0a0a0" margin="4"/>
  </style>
  
  <style name="5:center_ap">
    <normal text_color="green" text_align_h="center" border_color="#a0a0a0" font_name="ap" font_size="12"/>
  </style>

</label>

<progress_bar>
  <style>
    <normal bg_color="#f0f0f0" text_color="gold" fg_color="#c0c0c0" border_color="#a0a0a0" />
  </style>
  
  <style name="1:img" bg_image="slider_bg" fg_image="slider_fg">
    <normal icon="slider_drag"/>
  </style>

  <style name="2:img_v"  bg_image="slider_v_bg" fg_image="slider_v_fg" bg_image_draw_type="3patch_y" fg_image_draw_type="3patch_y">
    <normal icon="slider_drag"/>
  </style>
</progress_bar>

<slider>
  <style>
    <normal bg_color="#a0a0a0" fg_color="blue" border_color="#ffd700"/>
    <pressed bg_color="#a0a0a0" fg_color="blue" border_color="#ffa700"/>
    <over bg_color="#a0a0a0" fg_color="blue" border_color="#ffe700"/>
  </style>

  <style name="1:img" bg_image="slider_bg" fg_image="slider_fg">
    <normal icon="slider_drag"/>
    <pressed icon="slider_drag_p"/>
    <over icon="slider_drag_o"/>
  </style>

  <style name="2:img_v"  bg_image="slider_v_bg" fg_image="slider_v_fg" bg_image_draw_type="3patch_y" fg_image_draw_type="3patch_y">
    <normal icon="slider_drag"/>
    <pressed icon="slider_drag_p"/>
    <over icon="slider_drag_o"/>
  </style>
</slider>

<group_box>
  <style>
    <normal />
  </style>

  <style name="1:box">
    <normal bg_color="#f0f0f0" border_color="#a0a0a0" />
  </style>
</group_box>

<check_button bg_color="#f0f0f0" text_color="black">
  <style name="default">
    <checked   icon="checked" />
    <unchecked icon="unchecked" />
  </style>
  <style name="1:mute" >
    <checked   icon="unmuted" />
    <unchecked icon="muted" />
  </style>
</check_button>

<radio_button>
  <style name="default" bg_color="#f0f0f0" text_color="black">
    <checked   icon="radio_checked" />
    <unchecked icon="radio_unchecked" />
  </style>
  <style name="2:left">
    <checked   icon="left_on" />
    <unchecked icon="left_off" />
  </style>
  <style name="3:middle">
    <checked   icon="middle_on" />
    <unchecked icon="middle_off" />
  </style>
  <style name="4:right">
    <checked   icon="right_on" />
    <unchecked icon="right_off" />
  </style>
</radio_button>

<image>
  <style name="1:border">
    <normal border_color="#000000" bg_color="#e0e0e0"/>
  </style>
</image>

<dialog>
  <style name="default">
    <normal border_color="#606060" />
  </style>
</dialog>

<dialog_title icon="info" text_color="black">
  <style name="default">
    <normal bg_color="#a9a9a0" />
  </style>
  <style name="1:default1">
    <normal bg_image="dialog_title"  bg_image_draw_type="3patch_x"/>
  </style>
</dialog_title>

<dialog_client>
  <style name="default">
    <normal bg_color="#ededed" />
  </style>
  <style name="1:default1">
    <normal bg_color="#ededed" border_color="gray" />
  </style>
</dialog_client>

<scroll_bar>
  <style name="default">
    <normal bg_color="#c0c0c0" fg_color="#808080"/>
    <over bg_color="#c0c0c0" fg_color="#808080"/>
    <pressed bg_color="#c0c0c0" fg_color="#808080"/>
  </style>
  <style name="1:destkop" border_color="gray">
    <normal bg_color="#f0f0f0"/>
    <over bg_color="#f0f0f0" />
    <pressed bg_color="#f0f0f0" />
  </style>
</scroll_bar>

<dragger>
  <style name="1:scroll_bar" >
    <normal     bg_color="#c0c0c0" />
    <pressed    bg_color="#b0b0b0" />
    <over       bg_color="#a0a0a0" />
  </style>
<dragger>
